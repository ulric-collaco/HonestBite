# Backend Environment Variables

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/nutrition_truth
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# AI/ML Configuration
HUGGING_FACE_API_KEY=hf_your-token-here
HF_TOKEN=hf_your-token-here
HF_MODEL_NAME=distilbert-base-uncased
HF_CACHE_DIR=/tmp/huggingface_cache

# External APIs
OPEN_FOOD_FACTS_API_URL=https://world.openfoodfacts.org/api/v0

# Server Configuration
NODE_ENV=development
PORT=3001
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true
